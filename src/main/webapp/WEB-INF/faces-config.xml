<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>createPassengerBean</managed-bean-name>
		<managed-bean-class>ch.hevs.managedbeans.CreatePassengerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createAirportBean</managed-bean-name>
		<managed-bean-class>ch.hevs.managedbeans.CreateAirportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>flightManagerBean</managed-bean-name>
		<managed-bean-class>ch.hevs.managedbeans.FlightManagerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>welcomeTravel.xhtml</display-name>
		<from-view-id>/welcomeTravel.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bookFly</from-outcome>
			<to-view-id>/bookFly.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcomeTravel.xhtml</display-name>
		<from-view-id>/welcomeTravel.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createPassenger</from-outcome>
			<to-view-id>/createPassenger.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
		<display-name>createPassenger.xhtml</display-name>
		<from-view-id>/createPassenger.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>#{CreatePassengerBean.createPassenger}</from-outcome>
			<to-view-id>/showCreatePassengerResult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
			<display-name>showCreatePassengerResult.xhtml</display-name>
			<from-view-id>/showCreatePassengerResult.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>welcomeTravel</from-outcome>
				<to-view-id>/welcomeTravel.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>welcomeTravel.xhtml</display-name>
			<from-view-id>/welcomeTravel.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>createDepartureAirport</from-outcome>
				<to-view-id>/createDepartureAirport.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>createDepartureAirport.xhtml</display-name>
			<from-view-id>/createDepartureAirport.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>#{CreateAirportBean.createDepartureAirport}</from-outcome>
				<to-view-id>/showCreateAirportResult.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>welcomeTravel.xhtml</display-name>
			<from-view-id>/welcomeTravel.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>createArrivalAirport</from-outcome>
				<to-view-id>/createArrivalAirport.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>createArrivalAirport.xhtml</display-name>
			<from-view-id>/createArrivalAirport.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>#{CreateAirportBean.createArrivalAirport}</from-outcome>
				<to-view-id>/showCreateAirportResult.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>welcomeTravel.xhtml</display-name>
			<from-view-id>/welcomeTravel.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>createFlight</from-outcome>
				<to-view-id>/createFlight.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>createFlight.xhtml</display-name>
			<from-view-id>/createFlight.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>#{FlightManagerBean.createFlight}</from-outcome>
				<to-view-id>/showCreateFlightResult.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>showCreateAirportResult.xhtml</display-name>
			<from-view-id>/showCreateAirportResult.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>welcomeTravel</from-outcome>
				<to-view-id>/welcomeTravel.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>showCreateFlightResult.xhtml</display-name>
			<from-view-id>/showCreateFlightResult.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>welcomeTravel</from-outcome>
				<to-view-id>/welcomeTravel.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<display-name>bookFly.xhtml</display-name>
			<from-view-id>/bookFly.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>#{FlightManagerBean.flightSelection}</from-outcome>
				<to-view-id>/bookFlySelectFlight.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>

</faces-config>
